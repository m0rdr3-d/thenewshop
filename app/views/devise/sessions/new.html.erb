<div class="requirement" id="requirements"> <!-- Requirements Starts   -->
	<div class="container">
		<div class="all-require">
		
				<div class="heading">
					<h3>Log<span>  In</span> </h3>
				</div>
		
				<div class="specification" >
		
		<div class="col-md-4 require-size">
						<div class="button-grid">
							
						</div>
					</div>

					<div class="col-md-4 require-size">
						<div class="grid">
							

<div class="container">

  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  
  <div class="formHolder signup">

    <div class="form-group" >
        <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "form-control simplebox", id: "login", placeholder: "Email", style: 'width: 28%; margin-top: 5%;' %>
    </div>
      
    <div class="form-group">
        <%= f.password_field :password, autocomplete: "current-password", class: "form-control simplebox", id: "password", placeholder: "Password", style: 'width: 28%;' %>
    </div>

    <div class="form-group">
        <%= f.submit "Log in", class: "form-control simplebox", id: "signup", style: 'width:28%; margin-top: 3%; background: #f7de29;' %>
    </div>
    
  </div>
  <%end%>
  
</div>



						</div>
